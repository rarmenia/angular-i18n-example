{
  "hosting": {
    "public": "./dist/ng18n-example",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/fr/**",
        "destination": "/fr/index.html"
      },
      {
        "source": "/en-US/**",
        "destination": "/en-Us/index.html"
      },
      {
        "source": "/ja/**",
        "destination": "/ja/index.html"
      }
    ],
    "ng18n-example": {
      "root": "/"
    }
  }
}
